!!!
%html
  %head
    %title Atlrug2
    
    = stylesheet_link_tag 'screen.css', :media => 'screen, projection'
    = stylesheet_link_tag 'print.css', :media => 'print'
    /[if IE]
      = stylesheet_link_tag 'ie.css', :media => 'screen, projection'

    - javascript_include_tag :defaults
    = csrf_meta_tag
    
  %body
    #sidebar
      #masthead
        %a{:href => '/'}
          %img{:src => '/images/gem-small.png', :alt => 'Atlanta Ruby Logo' }
      #navigation
        %ul
          %li= link_to 'Meetings (on Meetup.com)', 'http://www.meetup.com/atlantaruby/'
          %li= link_to 'Videos (on Blip.tv)', 'http://skiptree.blip.tv'
          %li= link_to 'Work', '/work'
          %li= link_to 'Sponsors', '/sponsors'
    #main= yield
